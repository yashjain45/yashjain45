import array
a1=array.array('i',[])
a2=array.array('i',[])
n1=int(input("Enter size of array 1 ="))
n2=int(input("Enter size of array 2 ="))
print("Enter array1 elements:")
for i in range(0,n1):
    a1.append(int(input()))
print("array 2 elements:")
for i in range(0,n2):
    a2.append(int(input()))
print(a1+a2)
